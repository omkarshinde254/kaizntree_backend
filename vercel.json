{
    "builds": [
        {
            "src": "kaizntree_backend/wsgi.py",
            "use": "@ardnt/vercel-python-wsgi",
            "config": { "maxLambdaSize": "15mb" }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "kaizntree_backend/wsgi.py"
        }
    ]
}
